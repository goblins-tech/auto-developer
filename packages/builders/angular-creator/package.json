{
  "schematics": "./collection.json",
  "name": "@xxyyzz2050/angular-creator",
  "version": "1.0.3",
  "description": "installs Angular (a front-end framework https://angular.io), and material-design for Angular, and directly generates Angular modules and components",
  "main": "index.js",
  "private": false,
  "scripts": {
    "build": "if exist dist rmdir dist /s /Q && tsc -p tsconfig.json",
    "build:watch": "npm run build --watch",
    "example": "schematics .:init --app=ngProject --path=/myProject --dry-run=false --force=true",
    "example2": "schematics .:init",
    "example3": "node dist/index-spec",
    "publish": "npm run build && npm publish --access=public",
    "pack": "npm run build && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xxyyzz2050/creator.git"
  },
  "keywords": [
    "creator-builder",
    "angular-creator",
    "angular",
    "material design"
  ],
