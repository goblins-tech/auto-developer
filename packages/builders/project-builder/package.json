{
  "schematics": "./collection.json",
  "name": "@goblins-tech/project-builder",
  "version": "1.0.8",
  "description": "init a new project with package.json including the project info and the author info, and set up the basic settings for tsconfig, gitignore, ...",
  "main": "index.js",
  "private": false,
  "scripts": {
    "build": "if exist dist rmdir dist /s /Q && tsc -p tsconfig.json",
    "build:watch": "npm run build --watch",
    "example": "schematics .:init --name=myProject --path=/myProject --dry-run=false --force=true",
    "example:test": "schematics .:init",
    "example2": "node dist/index-spec",
    "publish": "npm run build && npm publish --access=public",
    "pack": "npm run build && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goblins-tech/autoDeveloper.git"
  },
  "keywords": [
    "autoDeveloper-builder",
    "project-builder",
    "developer",
    "init",
    "project",
    "workspace"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goblins-tech/autoDeveloper/issues"
  },
  "homepage": "https://github.com/goblins-tech/autoDeveloper#readme"
}
