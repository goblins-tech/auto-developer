{
  "schematics": "./schematics.json",
  "name": "@goblins-tech/angular-builder",
  "version": "1.0.3",
  "description": "installs Angular (a front-end framework https://angular.io), and material-design for Angular, and directly generates Angular modules and components",
  "main": "index.js",
  "private": false,
  "scripts": {
    "start": "schematics .:init  --dry-run=false --force=true",
    "example": "schematics .:init --app=ngProject --name=myProject --dry-run=false --force=true",
    "build": "tsc -p tsconfig.json",
    "build:watch": "npm run build --watch",
    "npm:publish": "npm run build && npm version patch && npm publish --access=public",
    "npm:pack": "npm run build && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goblins-tech/auto-developer.git"
  },
  "keywords": [
    "auto-developer",
    "auto-developer-builder",
    "angular-builder",
    "angular",
    "material design"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goblins-tech/auto-developer/issues"
  },
  "homepage": "https://github.com/goblins-tech/auto-developer#readme",
  "dependencies": {}
}
