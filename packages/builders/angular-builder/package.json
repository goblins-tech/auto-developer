{
  "schematics": "./collection.json",
  "name": "@goblins-tech/angular-builder",
  "version": "1.0.3",
  "description": "installs Angular (a front-end framework https://angular.io), and material-design for Angular, and directly generates Angular modules and components",
  "main": "index.js",
  "private": false,
  "scripts": {
    "build": "if exist dist rmdir dist /s /Q && tsc -p tsconfig.json",
    "build:watch": "npm run build --watch",
    "example": "schematics .:init --apps[]=ngProject --path=/myProject --dry-run=false --force=true",
    "example:test": "schematics .:init",
    "example3": "node dist/index-spec",
    "publish": "npm run build && npm publish --access=public",
    "pack": "npm run build && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goblins-tech/autoDeveloper.git"
  },
  "keywords": [
    "autoDeveloper-builder",
    "angular-builder",
    "angular",
    "material design"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goblins-tech/autoDeveloper/issues"
  },
  "homepage": "https://github.com/goblins-tech/autoDeveloper#readme",
  "devDependencies": {
    "@goblins-tech/auto-developer": ""
  }
}
