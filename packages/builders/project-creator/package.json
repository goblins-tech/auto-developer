{
  "schematics": "./collection.json",
  "name": "@xxyyzz2050/project-creator",
  "version": "1.0.3",
  "description": "init a new project with package.json including the project info and the author info, and set up the basic settings for tsconfig, gitignore, ...",
  "main": "index.js",
  "private": false,
  "scripts": {
    "build": "if exist dist rmdir dist /s /Q && tsc -p tsconfig.json",
    "build:watch": "npm run build --watch",
    "example": "schematics .:init --name=myProject --path=/myProject --dry-run=false --force=true",
    "example2": "schematics .:init",
    "example3": "node dist/index-spec",
    "publish": "npm run build && npm publish --access=public",
    "pack": "npm run build && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xxyyzz2050/creator.git"
  },
  "keywords": [
    "creator",
    "developer",
    "init",
    "project",
    "workspace"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xxyyzz2050/creator/issues"
  },
  "homepage": "https://github.com/xxyyzz2050/creator#readme"
}
