{
  "schematics": "./collection.json",
  "name": "@goblins-tech/auto-developer",
  "version": "1.0.0",
  "description": "automatically create any type of applications with any programming language, without coding",
  "main": "index.js",
  "private": false,
  "scripts": {
    "build": "if exist dist rmdir dist /s /Q && tsc -p tsconfig.json",
    "build:watch": "npm run build --watch",
    "example": "schematics .:build --name=myProject --path=/myProject --dry-run=false --force=true",
    "example2": "schematics .:build",
    "example3": "node dist/index-spec",
    "publish": "npm run build && npm publish --access=public",
    "pack": "npm run build && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goblins-tech/autoDeveloper.git"
  },
  "keywords": [
    "autoDeveloper",
    "developer",
    "angular",
    "react",
    "vue"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goblins-tech/autoDeveloper/issues"
  },
  "homepage": "https://github.com/goblins-tech/autoDeveloper#readme"
}
